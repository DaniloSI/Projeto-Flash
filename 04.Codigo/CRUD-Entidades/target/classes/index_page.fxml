<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="CENTER" hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Main">
  <children>
    <VBox prefHeight="600.0" prefWidth="800.0" GridPane.columnIndex="0" GridPane.rowIndex="0">
      <children>
        <MenuBar blendMode="SRC_OVER" cache="true" mouseTransparent="false" pickOnBounds="false" VBox.vgrow="ALWAYS">
          <menus>
            <Menu mnemonicParsing="false" text="Cadastros">
              <items>
                <MenuItem fx:id="menuCadastrosAreaConhecimento" mnemonicParsing="false" text="Área de Conhecimento" />
                <MenuItem fx:id="menuCadastrosCurso" mnemonicParsing="false" text="Curso" />
                <MenuItem fx:id="menuCadastrosDisciplina" mnemonicParsing="false" onAction="#handleMenuItemCadastrosDisicplina" text="Disciplina" />
                <MenuItem fx:id="menuCadastrosProfessor" mnemonicParsing="false" onAction="#handleMenuItemCadastrosProfessor" text="Professor" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Processos">
              <items>
                <MenuItem fx:id="menuProcessosAlocarProfessores" mnemonicParsing="false" text="Alocar Professores" />
                <MenuItem fx:id="menuProcessosIniciarGeracaoHorarios" mnemonicParsing="false" text="Iniciar geração de horários" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Relatórios">
              <items>
                <MenuItem fx:id="menuRelatoriosVisualizarRelatorios" mnemonicParsing="false" text="Visualizar Relatórios" />
              </items>
            </Menu>
          </menus>
        </MenuBar>
        <AnchorPane fx:id="anchorPane" prefHeight="576.0" prefWidth="800.0" />
      </children>
    </VBox>
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
</GridPane>
